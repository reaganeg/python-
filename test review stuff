#!/usr/bin/python3


def logme(func):
    def inner(*args,**kwargs):
        with open('test.log','wa') as fp:
            fp.write('calling {} \n'.format(func.__name__))
            a = func(*args,**kwargs)
            fp.write('returning {}{}\n'.format(func.__name__,a))
            return a
       
    return inner
@logme

def add(a,b):
    return a + b

if __name__ == '__main__':



